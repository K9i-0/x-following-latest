{
	"manifest_version": 3,
	"name": "[Dev] Bye Bye For You - X",
	"version": "1.2.0",
	"description": "X.comで「フォロー中」タブをデフォルトにし、最新順で表示します",
	"permissions": [],
	"content_scripts": [
		{
			"matches": [
				"*://twitter.com/*",
				"*://x.com/*"
			],
			"css": [
				"content.css"
			],
			"js": [
				"content.js"
			],
			"run_at": "document_idle"
		}
	],
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"action": {
		"default_icon": "icons/icon48.png"
	}
}
